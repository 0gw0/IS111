   "source": [
    "def print_square(symbol, size):\n",
    "    print(symbol * size)\n",
    "    n = size - 2\n",
    "    empty = \" \" * n\n",
    "    middle = (symbol + empty + symbol + \"\\n\") * n\n",
    "    print(middle + symbol * size)\n",
    "\n",
    "print_square(\"*\", 5)"
   ]
  }
